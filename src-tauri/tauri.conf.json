{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "dray",
  "version": "1.0.0",
  "identifier": "dray",
  "build": {
	"beforeDevCommand": "pnpm dev",
	"devUrl": "http://localhost:1420",
	"beforeBuildCommand": "pnpm build",
	"frontendDist": "../dist"
  },
  "app": {
	"security": {
	  "csp": null
	}
  },
  "bundle": {
	"resources": [
	  "ray/*"
	],
	"active": true,
	"targets": [
	  "dmg",
	  "nsis",
	  "deb",
	  "rpm"
	],
	"icon": [
	  "icons/64x64.png",
	  "icons/icon.png",
	  "icons/icon.icns",
	  "icons/icon.ico"
	]
  }
}
